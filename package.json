{
  "name": "cosmoscost",
  "displayName": "CosmosCost",
  "description": "CosmosCost - A beautiful color theme for VS Code",
  "version": "0.0.2",
  "publisher": "cosmoscost",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "color-theme",
    "cosmoscost",
    "dark",
    "light"
  ],
  "contributes": {
    "themes": [
      {
        "label": "CosmosCost Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/cosmoscost-dark-theme.json"
      },
      {
        "label": "CosmosCost Light",
        "uiTheme": "vs",
        "path": "./themes/cosmoscost-light-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmoscost/vscode-theme.git"
  },
  "icon": "assets/images/icon.png",
  "galleryBanner": {
    "color": "#0f172a",
    "theme": "dark"
  },
  "license": "MIT",
  "scripts": {
    "package": "vsce package",
    "package:analyze": "vsce package --out test.vsix && vsce ls test.vsix && du -h test.vsix && rm test.vsix",
    "package:size": "vsce package --out test.vsix && du -h test.vsix | cut -f1 && rm test.vsix"
  }
}
