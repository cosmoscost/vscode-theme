{
    "name": "CosmosCost Dark",
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "activityBar.activeBorder": "#60a5fa",
        "activityBar.background": "#0f172a",
        "activityBar.border": "#1e293b",
        "activityBar.foreground": "#60a5fa",
        "activityBar.inactiveForeground": "#64748b",
        "activityBarBadge.background": "#3b82f6",
        "activityBarBadge.foreground": "#ffffff",
        "badge.background": "#3b82f6",
        "badge.foreground": "#ffffff",
        "button.background": "#3b82f6",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#60a5fa",
        "diffEditor.insertedTextBackground": "#22c55e33",
        "diffEditor.removedTextBackground": "#ef444433",
        "dropdown.background": "#0f172a",
        "dropdown.border": "#1e293b",
        "dropdown.foreground": "#e2e8f0",
        "editor.background": "#0a0a0a",
        "editor.findMatchBackground": "#3b82f680",
        "editor.findMatchHighlightBackground": "#3b82f640",
        "editor.findRangeHighlightBackground": "#3b82f640",
        "editor.foreground": "#ededed",
        "editor.inactiveSelectionBackground": "#3b82f620",
        "editor.lineHighlightBackground": "#1e293b40",
        "editor.lineHighlightBorder": "#1e293b00",
        "editor.selectionBackground": "#3b82f640",
        "editor.selectionHighlightBackground": "#3b82f630",
        "editor.wordHighlightBackground": "#60a5fa40",
        "editor.wordHighlightStrongBackground": "#3b82f680",
        "editorBracketMatch.border": "#60a5fa",
        "editorCursor.foreground": "#60a5fa",
        "editorError.border": "#00000000",
        "editorError.foreground": "#ef4444",
        "editorGroup.border": "#1e293b",
        "editorGroup.dropBackground": "#3b82f640",
        "editorGroupHeader.tabsBackground": "#0f172a",
        "editorGroupHeader.tabsBorder": "#1e293b",
        "editorGutter.addedBackground": "#22c55e",
        "editorGutter.deletedBackground": "#ef4444",
        "editorGutter.modifiedBackground": "#f97316",
        "editorHoverWidget.background": "#1e293b",
        "editorHoverWidget.border": "#334155",
        "editorIndentGuide.activeBackground1": "#475569",
        "editorIndentGuide.background1": "#334155",
        "editorLineNumber.foreground": "#475569",
        "editorLineNumber.activeForeground": "#94a3b8",
        "editorLink.activeForeground": "#60a5fa",
        "editorSuggestWidget.background": "#1e293b",
        "editorSuggestWidget.border": "#334155",
        "editorSuggestWidget.foreground": "#e2e8f0",
        "editorSuggestWidget.highlightForeground": "#60a5fa",
        "editorSuggestWidget.selectedBackground": "#334155",
        "editorWarning.border": "#00000000",
        "editorWarning.foreground": "#f97316",
        "editorWhitespace.foreground": "#334155",
        "editorWidget.background": "#1e293b",
        "editorWidget.border": "#334155",
        "errorForeground": "#ef4444",
        "focusBorder": "#3b82f680",
        "foreground": "#ededed",
        "gitDecoration.conflictingResourceForeground": "#a78bfa",
        "gitDecoration.deletedResourceForeground": "#ef4444",
        "gitDecoration.ignoredResourceForeground": "#64748b",
        "gitDecoration.modifiedResourceForeground": "#f97316",
        "gitDecoration.untrackedResourceForeground": "#22c55e",
        "input.background": "#0f172a",
        "input.border": "#334155",
        "input.foreground": "#e2e8f0",
        "inputOption.activeBorder": "#3b82f6",
        "list.activeSelectionBackground": "#3b82f640",
        "list.activeSelectionForeground": "#60a5fa",
        "list.focusBackground": "#334155",
        "list.focusForeground": "#60a5fa",
        "list.highlightForeground": "#60a5fa",
        "list.hoverBackground": "#1e293b",
        "list.hoverForeground": "#e2e8f0",
        "list.inactiveSelectionBackground": "#1e293b",
        "list.inactiveSelectionForeground": "#94a3b8",
        "panel.background": "#0a0a0a",
        "panel.border": "#1e293b",
        "panelTitle.activeBorder": "#60a5fa",
        "panelTitle.activeForeground": "#e2e8f0",
        "progressBar.background": "#3b82f6",
        "scrollbar.shadow": "#00000080",
        "scrollbarSlider.activeBackground": "#3b82f680",
        "scrollbarSlider.background": "#33415540",
        "scrollbarSlider.hoverBackground": "#33415560",
        "selection.background": "#3b82f680",
        "sideBar.background": "#0f172a",
        "sideBar.border": "#1e293b",
        "sideBar.foreground": "#cbd5e1",
        "sideBarSectionHeader.background": "#1e293b",
        "sideBarSectionHeader.foreground": "#e2e8f0",
        "sideBarTitle.foreground": "#94a3b8",
        "statusBar.background": "#0f172a",
        "statusBar.border": "#1e293b",
        "statusBar.foreground": "#94a3b8",
        "statusBar.debuggingBackground": "#f97316",
        "statusBar.noFolderBackground": "#0f172a",
        "statusBarItem.activeBackground": "#3b82f640",
        "tab.activeBackground": "#0a0a0a",
        "tab.activeBorderTop": "#60a5fa",
        "tab.activeForeground": "#60a5fa",
        "tab.border": "#1e293b",
        "tab.inactiveBackground": "#0f172a",
        "tab.inactiveForeground": "#64748b",
        "terminal.ansiBlack": "#0f172a",
        "terminal.ansiBlue": "#3b82f6",
        "terminal.ansiBrightBlack": "#475569",
        "terminal.ansiBrightBlue": "#60a5fa",
        "terminal.ansiBrightCyan": "#06b6d4",
        "terminal.ansiBrightGreen": "#22c55e",
        "terminal.ansiBrightMagenta": "#a78bfa",
        "terminal.ansiBrightRed": "#f87171",
        "terminal.ansiBrightWhite": "#f1f5f9",
        "terminal.ansiBrightYellow": "#fb923c",
        "terminal.ansiCyan": "#0891b2",
        "terminal.ansiGreen": "#16a34a",
        "terminal.ansiMagenta": "#8b5cf6",
        "terminal.ansiRed": "#dc2626",
        "terminal.ansiWhite": "#cbd5e1",
        "terminal.ansiYellow": "#f97316",
        "terminal.background": "#0a0a0a",
        "terminal.foreground": "#e2e8f0",
        "terminal.selectionBackground": "#3b82f640",
        "terminalCursor.background": "#0a0a0a",
        "terminalCursor.foreground": "#60a5fa",
        "titleBar.activeBackground": "#0f172a",
        "titleBar.activeForeground": "#e2e8f0",
        "titleBar.border": "#1e293b",
        "titleBar.inactiveBackground": "#0f172a",
        "titleBar.inactiveForeground": "#64748b",
        "tree.indentGuidesStroke": "#334155",
        "widget.shadow": "#00000080"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#64748b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "markup.inserted",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#22c55e"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character.escape",
                "constant"
            ],
            "settings": {
                "foreground": "#fb923c"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "storage",
                "keyword.control.flow",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#8b5cf6"
            }
        },
        {
            "scope": [
                "variable",
                "variable.language",
                "variable.parameter",
                "variable.other.property",
                "variable.object.property"
            ],
            "settings": {
                "foreground": "#e2e8f0"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#60a5fa"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class",
                "support.type",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#06b6d4"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#3b82f6"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "meta.object-literal.key",
                "string.unquoted",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "scope": [
                "meta.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#64748b"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal",
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#ef4444"
            }
        },
        {
            "scope": [
                "markup.underline",
                "markup.underline.link"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#60a5fa"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3b82f6"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#f97316"
            }
        },
        {
            "scope": [
                "support",
                "entity"
            ],
            "settings": {
                "foreground": "#fb923c"
            }
        },
        {
            "scope": [
                "meta.type.annotation",
                "meta.parameters",
                "meta.type.parameters",
                "meta.return.type"
            ],
            "settings": {
                "foreground": "#06b6d4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#06b6d4"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#f97316"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#ef4444"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#ec4899"
            }
        }
    ]
}
