{
    "name": "CosmosCost Dark",
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "colors": {
        "focusBorder": "#60a5fa33",
        "foreground": "#ededed",
        "errorForeground": "#ef4444",
        "widget.shadow": "#0f0f0f",
        "selection.background": "#3b82f67f",
        "titleBar.activeBackground": "#0f172a",
        "titleBar.border": "#000000",
        "titleBar.inactiveBackground": "#1e293b",
        "statusBar.background": "#0f172a",
        "statusBar.foreground": "#94a3b8",
        "statusBar.border": "#000000",
        "statusBar.debuggingForeground": "#0f172a",
        "statusBar.debuggingBackground": "#f97316",
        "statusBarItem.activeBackground": "#a78bfa",
        "dropdown.background": "#0f172a",
        "dropdown.border": "#334155",
        "dropdown.foreground": "#e2e8f0",
        "input.background": "#0f172a",
        "input.border": "#334155",
        "input.foreground": "#e2e8f0",
        "input.placeholderForeground": "#a78bfa7f",
        "inputOption.activeBorder": "#a78bfa",
        "button.background": "#22c55e",
        "button.foreground": "#0f172a",
        "button.hoverBackground": "#16a34a",
        "list.activeSelectionBackground": "#1e293b",
        "list.activeSelectionForeground": "#a78bfa",
        "list.focusBackground": "#334155",
        "list.focusForeground": "#a78bfa",
        "list.highlightForeground": "#fb923c",
        "list.hoverBackground": "#334155",
        "list.hoverForeground": "#ededed",
        "list.inactiveSelectionBackground": "#1e293b",
        "list.inactiveSelectionForeground": "#a78bfa",
        "editor.background": "#0a0a0a",
        "editor.foreground": "#ededed",
        "editorLineNumber.foreground": "#60a5fa33",
        "editorIndentGuide.activeBackground": "#64748b",
        "editorCursor.foreground": "#a78bfa",
        "editor.selectionBackground": "#3b82f67f",
        "editor.selectionHighlightBackground": "#3b82f67f",
        "editor.inactiveSelectionBackground": "#3b82f67f",
        "editor.wordHighlightBackground": "#3b82f67f",
        "editor.wordHighlightStrongBackground": "#3b82f67f",
        "editor.findMatchBackground": "#3b82f67f",
        "editor.findMatchHighlightBackground": "#3b82f67f",
        "editor.findRangeHighlightBackground": "#3b82f67f",
        "editor.lineHighlightBackground": "#60a5fa33",
        "editor.lineHighlightBorder": "#60a5fa00",
        "editor.stackFrameHighlightBackground": "#353424",
        "editorInlayHint.background": "#1e293b",
        "editorInlayHint.foreground": "#e2e8f0",
        "editorLink.activeForeground": "#a78bfa",
        "editorWhitespace.foreground": "#334155",
        "editorIndentGuide.background": "#334155",
        "editorBracketMatch.border": "#a78bfa",
        "editorError.foreground": "#ef4444",
        "editorError.border": "#ffffff00",
        "editorWarning.foreground": "#fb923c",
        "editorWarning.border": "#ffffff00",
        "editorGutter.modifiedBackground": "#fb923c",
        "editorGutter.addedBackground": "#22c55e",
        "editorGutter.deletedBackground": "#ef4444",
        "editorWidget.background": "#0f172a",
        "editorWidget.border": "#334155",
        "editorSuggestWidget.background": "#0f172a",
        "editorSuggestWidget.border": "#334155",
        "editorSuggestWidget.foreground": "#e2e8f0",
        "editorSuggestWidget.highlightForeground": "#22c55e",
        "editorSuggestWidget.selectedBackground": "#475569",
        "editorHoverWidget.background": "#0f172a",
        "editorHoverWidget.border": "#334155",
        "editorGroup.border": "#000000",
        "editorGroup.dropBackground": "#3b82f67f",
        "editorGroupHeader.tabsBackground": "#0f172a",
        "editorGroupHeader.tabsBorder": "#000000",
        "tab.activeBackground": "#00000000",
        "tab.activeForeground": "#22c55e",
        "tab.border": "#000000",
        "tab.inactiveBackground": "#0f172a",
        "tab.inactiveForeground": "#64748b",
        "tab.activeBorderTop": "#22c55e",
        "panel.border": "#000000",
        "panelTitle.activeBorder": "#22c55e",
        "panelTitle.activeForeground": "#e2e8f0",
        "activityBar.background": "#0f172a",
        "activityBar.foreground": "#22c55e",
        "activityBar.inactiveForeground": "#475569",
        "activityBar.border": "#000000",
        "activityBarBadge.background": "#a78bfa",
        "activityBarBadge.foreground": "#0f172a",
        "activityBar.activeBorder": "#a78bfa",
        "badge.foreground": "#0f172a",
        "badge.background": "#a78bfa",
        "sideBar.background": "#0a0a0a",
        "sideBar.foreground": "#e2e8f0",
        "sideBar.border": "#000000",
        "sideBarTitle.foreground": "#94a3b8",
        "sideBarSectionHeader.background": "#0f172a",
        "sideBarSectionHeader.foreground": "#94a3b8",
        "progressBar.background": "#22c55e",
        "scrollbar.shadow": "#0f172a",
        "scrollbarSlider.activeBackground": "#3b82f67f",
        "scrollbarSlider.background": "#60a5fa33",
        "scrollbarSlider.hoverBackground": "#60a5fa33",
        "terminal.background": "#0f172a",
        "terminal.foreground": "#e2e8f0",
        "terminal.ansiBlack": "#0f172a",
        "terminal.ansiBlue": "#a78bfa",
        "terminal.ansiBrightBlack": "#334155",
        "terminal.ansiBrightBlue": "#a78bfa",
        "terminal.ansiBrightCyan": "#22c55e",
        "terminal.ansiBrightGreen": "#a78bfa",
        "terminal.ansiBrightMagenta": "#22c55e",
        "terminal.ansiBrightRed": "#fb923c",
        "terminal.ansiBrightWhite": "#ededed",
        "terminal.ansiBrightYellow": "#fb923c",
        "terminal.ansiCyan": "#a78bfa",
        "terminal.ansiGreen": "#22c55e",
        "terminal.ansiMagenta": "#22c55e",
        "terminal.ansiRed": "#ef4444",
        "terminal.ansiWhite": "#e2e8f0",
        "terminal.ansiYellow": "#fb923c",
        "terminal.selectionBackground": "#3b82f67f",
        "terminalCursor.background": "#334155",
        "terminalCursor.foreground": "#94a3b8",
        "gitDecoration.modifiedResourceForeground": "#fb923c",
        "gitDecoration.deletedResourceForeground": "#ef4444",
        "gitDecoration.untrackedResourceForeground": "#22c55e",
        "gitDecoration.ignoredResourceForeground": "#475569",
        "gitDecoration.conflictingResourceForeground": "#a78bfa",
        "diffEditor.insertedTextBackground": "#22c55e23",
        "diffEditor.removedTextBackground": "#ef444420",
        "tree.indentGuidesStroke": "#475569"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#64748b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "markup.inserted",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#22c55e"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character.escape",
                "constant"
            ],
            "settings": {
                "foreground": "#fb923c"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "storage",
                "keyword.control.flow",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#a78bfa"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#8b5cf6"
            }
        },
        {
            "scope": [
                "variable",
                "variable.language",
                "variable.parameter",
                "variable.other.property",
                "variable.object.property"
            ],
            "settings": {
                "foreground": "#e2e8f0"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#60a5fa"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class",
                "support.type",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#06b6d4"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#3b82f6"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "meta.object-literal.key",
                "string.unquoted",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "scope": [
                "meta.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#64748b"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#cbd5e1"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal",
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#ef4444"
            }
        },
        {
            "scope": [
                "markup.underline",
                "markup.underline.link"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#60a5fa"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#3b82f6"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#f97316"
            }
        },
        {
            "scope": [
                "support",
                "entity"
            ],
            "settings": {
                "foreground": "#fb923c"
            }
        },
        {
            "scope": [
                "meta.type.annotation",
                "meta.parameters",
                "meta.type.parameters",
                "meta.return.type"
            ],
            "settings": {
                "foreground": "#06b6d4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#06b6d4"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#f97316"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#ef4444"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#ec4899"
            }
        }
    ]
}
