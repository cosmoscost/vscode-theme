{
    "name": "CosmosCost Light",
    "$schema": "vscode://schemas/color-theme",
    "type": "light",
    "colors": {
        "focusBorder": "#2563eb33",
        "foreground": "#171717",
        "errorForeground": "#dc2626",
        "widget.shadow": "#0000001a",
        "selection.background": "#3b82f67f",
        "titleBar.activeBackground": "#f8fafc",
        "titleBar.border": "#e2e8f0",
        "titleBar.inactiveBackground": "#e2e8f0",
        "statusBar.background": "#f8fafc",
        "statusBar.foreground": "#64748b",
        "statusBar.border": "#e2e8f0",
        "statusBar.debuggingForeground": "#f8fafc",
        "statusBar.debuggingBackground": "#ea580c",
        "statusBarItem.activeBackground": "#7c3aed",
        "dropdown.background": "#f8fafc",
        "dropdown.border": "#cbd5e1",
        "dropdown.foreground": "#1e293b",
        "input.background": "#f8fafc",
        "input.border": "#cbd5e1",
        "input.foreground": "#1e293b",
        "input.placeholderForeground": "#7c3aed7f",
        "inputOption.activeBorder": "#7c3aed",
        "button.background": "#ea580c",
        "button.foreground": "#f8fafc",
        "button.hoverBackground": "#c2410c",
        "list.activeSelectionBackground": "#e2e8f0",
        "list.activeSelectionForeground": "#7c3aed",
        "list.focusBackground": "#cbd5e1",
        "list.focusForeground": "#7c3aed",
        "list.highlightForeground": "#c2410c",
        "list.hoverBackground": "#cbd5e1",
        "list.hoverForeground": "#171717",
        "list.inactiveSelectionBackground": "#e2e8f0",
        "list.inactiveSelectionForeground": "#7c3aed",
        "editor.background": "#fafaf9",
        "editor.foreground": "#171717",
        "editorLineNumber.foreground": "#2563eb33",
        "editorIndentGuide.activeBackground": "#94a3b8",
        "editorCursor.foreground": "#7c3aed",
        "editor.selectionBackground": "#3b82f67f",
        "editor.selectionHighlightBackground": "#3b82f67f",
        "editor.inactiveSelectionBackground": "#3b82f67f",
        "editor.wordHighlightBackground": "#3b82f67f",
        "editor.wordHighlightStrongBackground": "#3b82f67f",
        "editor.findMatchBackground": "#3b82f67f",
        "editor.findMatchHighlightBackground": "#3b82f67f",
        "editor.findRangeHighlightBackground": "#3b82f67f",
        "editor.lineHighlightBackground": "#2563eb33",
        "editor.lineHighlightBorder": "#2563eb00",
        "editor.stackFrameHighlightBackground": "#f9fafb",
        "editorInlayHint.background": "#e2e8f0",
        "editorInlayHint.foreground": "#1e293b",
        "editorLink.activeForeground": "#7c3aed",
        "editorWhitespace.foreground": "#cbd5e1",
        "editorIndentGuide.background": "#cbd5e1",
        "editorBracketMatch.border": "#7c3aed",
        "editorError.foreground": "#dc2626",
        "editorError.border": "#ffffff00",
        "editorWarning.foreground": "#c2410c",
        "editorWarning.border": "#ffffff00",
        "editorGutter.modifiedBackground": "#c2410c",
        "editorGutter.addedBackground": "#ea580c",
        "editorGutter.deletedBackground": "#dc2626",
        "editorWidget.background": "#f8fafc",
        "editorWidget.border": "#cbd5e1",
        "editorSuggestWidget.background": "#f8fafc",
        "editorSuggestWidget.border": "#cbd5e1",
        "editorSuggestWidget.foreground": "#1e293b",
        "editorSuggestWidget.highlightForeground": "#ea580c",
        "editorSuggestWidget.selectedBackground": "#94a3b8",
        "editorHoverWidget.background": "#f8fafc",
        "editorHoverWidget.border": "#cbd5e1",
        "editorGroup.border": "#e2e8f0",
        "editorGroup.dropBackground": "#3b82f67f",
        "editorGroupHeader.tabsBackground": "#f8fafc",
        "editorGroupHeader.tabsBorder": "#e2e8f0",
        "tab.activeBackground": "#fafaf900",
        "tab.activeForeground": "#ea580c",
        "tab.border": "#e2e8f0",
        "tab.inactiveBackground": "#f8fafc",
        "tab.inactiveForeground": "#94a3b8",
        "tab.activeBorderTop": "#ea580c",
        "panel.border": "#e2e8f0",
        "panelTitle.activeBorder": "#ea580c",
        "panelTitle.activeForeground": "#1e293b",
        "activityBar.background": "#f8fafc",
        "activityBar.foreground": "#ea580c",
        "activityBar.inactiveForeground": "#94a3b8",
        "activityBar.border": "#e2e8f0",
        "activityBarBadge.background": "#7c3aed",
        "activityBarBadge.foreground": "#f8fafc",
        "activityBar.activeBorder": "#7c3aed",
        "badge.foreground": "#f8fafc",
        "badge.background": "#7c3aed",
        "sideBar.background": "#fafaf9",
        "sideBar.foreground": "#1e293b",
        "sideBar.border": "#e2e8f0",
        "sideBarTitle.foreground": "#64748b",
        "sideBarSectionHeader.background": "#f8fafc",
        "sideBarSectionHeader.foreground": "#64748b",
        "progressBar.background": "#ea580c",
        "scrollbar.shadow": "#f8fafc",
        "scrollbarSlider.activeBackground": "#3b82f67f",
        "scrollbarSlider.background": "#2563eb33",
        "scrollbarSlider.hoverBackground": "#2563eb33",
        "terminal.background": "#f8fafc",
        "terminal.foreground": "#1e293b",
        "terminal.ansiBlack": "#f8fafc",
        "terminal.ansiBlue": "#7c3aed",
        "terminal.ansiBrightBlack": "#cbd5e1",
        "terminal.ansiBrightBlue": "#7c3aed",
        "terminal.ansiBrightCyan": "#ea580c",
        "terminal.ansiBrightGreen": "#7c3aed",
        "terminal.ansiBrightMagenta": "#ea580c",
        "terminal.ansiBrightRed": "#c2410c",
        "terminal.ansiBrightWhite": "#171717",
        "terminal.ansiBrightYellow": "#c2410c",
        "terminal.ansiCyan": "#7c3aed",
        "terminal.ansiGreen": "#ea580c",
        "terminal.ansiMagenta": "#ea580c",
        "terminal.ansiRed": "#dc2626",
        "terminal.ansiWhite": "#1e293b",
        "terminal.ansiYellow": "#c2410c",
        "terminal.selectionBackground": "#3b82f67f",
        "terminalCursor.background": "#cbd5e1",
        "terminalCursor.foreground": "#64748b",
        "gitDecoration.modifiedResourceForeground": "#c2410c",
        "gitDecoration.deletedResourceForeground": "#dc2626",
        "gitDecoration.untrackedResourceForeground": "#ea580c",
        "gitDecoration.ignoredResourceForeground": "#94a3b8",
        "gitDecoration.conflictingResourceForeground": "#7c3aed",
        "diffEditor.insertedTextBackground": "#ea580c23",
        "diffEditor.removedTextBackground": "#dc262620",
        "tree.indentGuidesStroke": "#94a3b8"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#94a3b8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "storage",
                "support",
                "entity.name.tag",
                "variable.language",
                "keyword.control.flow",
                "storage.modifier",
                "keyword.operator",
                "entity.other.attribute-name.class.css",
                "entity.other.keyframe-offset",
                "markup.heading",
                "markup.underline.link",
                "variable.other.env",
                "punctuation.definition.list.begin.markdown",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.markdown",
                "punctuation.definition.quote.begin.markdown",
                "punctuation.definition.raw.markdown",
                "constant.length.units.css",
                "constant.percentage.units.css"
            ],
            "settings": {
                "foreground": "#7c3aed"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "markup.inserted",
                "markup.raw",
                "constant",
                "source.env",
                "support.type.builtin.graphql",
                "variable.other.quoted.double",
                "markup.inline.raw.string.markdown",
                "entity.other.attribute-name.id.css",
                "meta.jsx.children",
                "JSXNested"
            ],
            "settings": {
                "foreground": "#ea580c"
            }
        },
        {
            "name": "Function",
            "scope": [
                "markup.changed",
                "entity",
                "entity.name.function",
                "entity.name.function.elixir",
                "entity.name.function-call.elixir",
                "support.class.component.tsx",
                "support.class.component.open.jsx",
                "support.class.component.close.jsx",
                "meta.function-call.generic.python",
                "entity.name.section.markdown",
                "storage.type.annotation.dart",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#c2410c"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#dc2626"
            }
        },
        {
            "name": "Property",
            "scope": [
                "string.unquoted",
                "punctuation.separator",
                "entity.other.attribute-name",
                "meta.object-literal.key",
                "variable.object.property",
                "variable.other.property",
                "variable.other.object.property",
                "variable.other.constant.property",
                "meta.type.annotation",
                "support.type.property-name.css",
                "support.type.vendored",
                "constant.language.symbol.elixir",
                "variable.graphql",
                "meta.attribute.python",
                "source.dart"
            ],
            "settings": {
                "foreground": "#be185d"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "markup.list",
                "support.constant.property-value.css",
                "variable.parameter.keyframe-list.css",
                "source.css",
                "support.constant.font-name",
                "support.constant.vendored.property-value",
                "variable.parameter",
                "meta.class",
                "meta.method.declaration",
                "parameter.variable.function.elixir",
                "punctuation.definition.tag",
                "punctuation.section.embedded",
                "meta.embedded.expression",
                "punctuation.terminator.dart",
                "punctuation.dot.dart",
                "variable.language",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#171717"
            }
        },
        {
            "name": "Type/Class",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.class.builtin",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.quasi.element.begin",
                "punctuation.quasi.element.end",
                "entity.other.inherited-class",
                "variable.other.constant.elixir",
                "entity.other.attribute-name.class.css",
                "support.class.dart",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#0891b2"
            }
        },
        {
            "name": "Markup Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Bold",
            "scope": [
                "markup.bold.markdown",
                "storage.type.annotation.dart"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Italic",
            "scope": [
                "markup.italic",
                "meta.parameters",
                "meta.type.parameters",
                "meta.return.type",
                "entity.name.type.interface",
                "meta.type.annotation",
                "meta.function.parameters",
                "markup.italic.markdown"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        }
    ]
}
