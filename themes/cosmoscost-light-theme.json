{
    "name": "CosmosCost Light",
    "$schema": "vscode://schemas/color-theme",
    "type": "light",
    "colors": {
        "activityBar.activeBorder": "#2563eb",
        "activityBar.background": "#f8fafc",
        "activityBar.border": "#e2e8f0",
        "activityBar.foreground": "#2563eb",
        "activityBar.inactiveForeground": "#64748b",
        "activityBarBadge.background": "#2563eb",
        "activityBarBadge.foreground": "#ffffff",
        "badge.background": "#2563eb",
        "badge.foreground": "#ffffff",
        "button.background": "#2563eb",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#1e40af",
        "diffEditor.insertedTextBackground": "#22c55e33",
        "diffEditor.removedTextBackground": "#ef444433",
        "dropdown.background": "#ffffff",
        "dropdown.border": "#cbd5e1",
        "dropdown.foreground": "#1e293b",
        "editor.background": "#fafaf9",
        "editor.findMatchBackground": "#3b82f680",
        "editor.findMatchHighlightBackground": "#3b82f640",
        "editor.findRangeHighlightBackground": "#3b82f640",
        "editor.foreground": "#171717",
        "editor.inactiveSelectionBackground": "#3b82f620",
        "editor.lineHighlightBackground": "#f1f5f9",
        "editor.lineHighlightBorder": "#f1f5f900",
        "editor.selectionBackground": "#3b82f640",
        "editor.selectionHighlightBackground": "#3b82f630",
        "editor.wordHighlightBackground": "#3b82f640",
        "editor.wordHighlightStrongBackground": "#3b82f680",
        "editorBracketMatch.border": "#2563eb",
        "editorCursor.foreground": "#2563eb",
        "editorError.border": "#00000000",
        "editorError.foreground": "#dc2626",
        "editorGroup.border": "#e2e8f0",
        "editorGroup.dropBackground": "#3b82f640",
        "editorGroupHeader.tabsBackground": "#f8fafc",
        "editorGroupHeader.tabsBorder": "#e2e8f0",
        "editorGutter.addedBackground": "#16a34a",
        "editorGutter.deletedBackground": "#dc2626",
        "editorGutter.modifiedBackground": "#ea580c",
        "editorHoverWidget.background": "#ffffff",
        "editorHoverWidget.border": "#cbd5e1",
        "editorIndentGuide.activeBackground1": "#94a3b8",
        "editorIndentGuide.background1": "#e2e8f0",
        "editorLineNumber.foreground": "#94a3b8",
        "editorLineNumber.activeForeground": "#475569",
        "editorLink.activeForeground": "#2563eb",
        "editorSuggestWidget.background": "#ffffff",
        "editorSuggestWidget.border": "#cbd5e1",
        "editorSuggestWidget.foreground": "#1e293b",
        "editorSuggestWidget.highlightForeground": "#2563eb",
        "editorSuggestWidget.selectedBackground": "#f1f5f9",
        "editorWarning.border": "#00000000",
        "editorWarning.foreground": "#ea580c",
        "editorWhitespace.foreground": "#e2e8f0",
        "editorWidget.background": "#ffffff",
        "editorWidget.border": "#cbd5e1",
        "errorForeground": "#dc2626",
        "focusBorder": "#3b82f680",
        "foreground": "#171717",
        "gitDecoration.conflictingResourceForeground": "#7c3aed",
        "gitDecoration.deletedResourceForeground": "#dc2626",
        "gitDecoration.ignoredResourceForeground": "#94a3b8",
        "gitDecoration.modifiedResourceForeground": "#ea580c",
        "gitDecoration.untrackedResourceForeground": "#16a34a",
        "input.background": "#ffffff",
        "input.border": "#cbd5e1",
        "input.foreground": "#1e293b",
        "inputOption.activeBorder": "#2563eb",
        "list.activeSelectionBackground": "#dbeafe",
        "list.activeSelectionForeground": "#1e40af",
        "list.focusBackground": "#f1f5f9",
        "list.focusForeground": "#1e40af",
        "list.highlightForeground": "#2563eb",
        "list.hoverBackground": "#f8fafc",
        "list.hoverForeground": "#1e293b",
        "list.inactiveSelectionBackground": "#f1f5f9",
        "list.inactiveSelectionForeground": "#475569",
        "panel.background": "#fafaf9",
        "panel.border": "#e2e8f0",
        "panelTitle.activeBorder": "#2563eb",
        "panelTitle.activeForeground": "#1e293b",
        "progressBar.background": "#2563eb",
        "scrollbar.shadow": "#00000020",
        "scrollbarSlider.activeBackground": "#94a3b880",
        "scrollbarSlider.background": "#cbd5e140",
        "scrollbarSlider.hoverBackground": "#cbd5e160",
        "selection.background": "#3b82f680",
        "sideBar.background": "#f8fafc",
        "sideBar.border": "#e2e8f0",
        "sideBar.foreground": "#475569",
        "sideBarSectionHeader.background": "#f1f5f9",
        "sideBarSectionHeader.foreground": "#1e293b",
        "sideBarTitle.foreground": "#64748b",
        "statusBar.background": "#f8fafc",
        "statusBar.border": "#e2e8f0",
        "statusBar.foreground": "#64748b",
        "statusBar.debuggingBackground": "#ea580c",
        "statusBar.noFolderBackground": "#f8fafc",
        "statusBarItem.activeBackground": "#3b82f640",
        "tab.activeBackground": "#fafaf9",
        "tab.activeBorderTop": "#2563eb",
        "tab.activeForeground": "#2563eb",
        "tab.border": "#e2e8f0",
        "tab.inactiveBackground": "#f8fafc",
        "tab.inactiveForeground": "#64748b",
        "terminal.ansiBlack": "#1e293b",
        "terminal.ansiBlue": "#2563eb",
        "terminal.ansiBrightBlack": "#64748b",
        "terminal.ansiBrightBlue": "#3b82f6",
        "terminal.ansiBrightCyan": "#0891b2",
        "terminal.ansiBrightGreen": "#16a34a",
        "terminal.ansiBrightMagenta": "#7c3aed",
        "terminal.ansiBrightRed": "#dc2626",
        "terminal.ansiBrightWhite": "#f8fafc",
        "terminal.ansiBrightYellow": "#ea580c",
        "terminal.ansiCyan": "#0e7490",
        "terminal.ansiGreen": "#15803d",
        "terminal.ansiMagenta": "#6d28d9",
        "terminal.ansiRed": "#b91c1c",
        "terminal.ansiWhite": "#cbd5e1",
        "terminal.ansiYellow": "#c2410c",
        "terminal.background": "#fafaf9",
        "terminal.foreground": "#1e293b",
        "terminal.selectionBackground": "#3b82f640",
        "terminalCursor.background": "#fafaf9",
        "terminalCursor.foreground": "#2563eb",
        "titleBar.activeBackground": "#f8fafc",
        "titleBar.activeForeground": "#1e293b",
        "titleBar.border": "#e2e8f0",
        "titleBar.inactiveBackground": "#f8fafc",
        "titleBar.inactiveForeground": "#94a3b8",
        "tree.indentGuidesStroke": "#cbd5e1",
        "widget.shadow": "#00000020"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#64748b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template",
                "markup.inserted",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#15803d"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character.escape",
                "constant"
            ],
            "settings": {
                "foreground": "#ea580c"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier",
                "storage",
                "keyword.control.flow",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#7c3aed"
            }
        },
        {
            "scope": [
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression"
            ],
            "settings": {
                "foreground": "#6d28d9"
            }
        },
        {
            "scope": [
                "variable",
                "variable.language",
                "variable.parameter",
                "variable.other.property",
                "variable.object.property"
            ],
            "settings": {
                "foreground": "#1e293b"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function",
                "meta.function-call"
            ],
            "settings": {
                "foreground": "#1e40af"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type",
                "support.class",
                "support.type",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#0e7490"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#2563eb"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name",
                "meta.object-literal.key",
                "string.unquoted",
                "punctuation.separator"
            ],
            "settings": {
                "foreground": "#be185d"
            }
        },
        {
            "scope": [
                "meta.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#64748b"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#475569"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal",
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#dc2626"
            }
        },
        {
            "scope": [
                "markup.underline",
                "markup.underline.link"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#1e40af"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#2563eb"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#ea580c"
            }
        },
        {
            "scope": [
                "support",
                "entity"
            ],
            "settings": {
                "foreground": "#c2410c"
            }
        },
        {
            "scope": [
                "meta.type.annotation",
                "meta.parameters",
                "meta.type.parameters",
                "meta.return.type"
            ],
            "settings": {
                "foreground": "#0e7490",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#0891b2"
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#ea580c"
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#dc2626"
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#be185d"
            }
        }
    ]
}
